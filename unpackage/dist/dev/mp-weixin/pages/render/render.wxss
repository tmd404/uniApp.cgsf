.start{
	margin-top:20px;
	
	-webkit-animation: fadenum 100s linear infinite;
	
	        animation: fadenum 100s linear infinite;
}
@-webkit-keyframes fadenum{
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);
}}
@keyframes fadenum{
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);
}}
